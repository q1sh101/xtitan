{
  "name": "@xtitan/ui",
  "version": "0.0.0",
  "description": "Shared UI components for XTitan applications",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-native": "^0.70.0"
  },
  "dependencies": {
    "@xtitan/theme": "workspace:*"
  },
  "devDependencies": {
    "@xtitan/config": "workspace:*",
    "@types/react": "^18.3.12",
    "react": "^18.3.1",
    "react-native": "^0.76.9",
    "typescript": "^5.7.2"
  },
  "files": [
    "dist"
  ]
}